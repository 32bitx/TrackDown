{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["views/**"] }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.js"
    }
  ],
  "env": {
    "bot": "@telegram_bot_token"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
